{
    "name": "lawoole/framework",
    "description": "A fast framework with Laravel components based on Swoole.",
    "keywords": ["framework", "swoole", "laravel", "lawoole"],
    "license": "MIT",
    "homepage": "https://github.com/lawoole/framework",
    "support": {
        "issues": "https://github.com/lawoole/framework/issues",
        "source": "https://github.com/lawoole/framework"
    },
    "authors": [
        {
            "name": "You Ming",
            "email": "youming@funcuter.org"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/cache": "5.5.*",
        "illuminate/config": "5.5.*",
        "illuminate/console": "5.5.*",
        "illuminate/container": "5.5.*",
        "illuminate/contracts": "5.5.*",
        "illuminate/database": "5.5.*",
        "illuminate/events": "5.5.*",
        "illuminate/filesystem": "5.5.*",
        "illuminate/http": "5.5.*",
        "illuminate/log": "5.5.*",
        "illuminate/pipeline": "5.5.*",
        "illuminate/support": "5.5.*",
        "symfony/debug": "~3.3",
        "symfony/http-foundation": "~3.3",
        "symfony/http-kernel": "~3.3",
        "symfony/process": "~3.3",
        "nikic/fast-route": "~1.2",
        "monolog/monolog": "~1.23"
    },
    "replace": {
        "lawoole/console": "self.version",
        "lawoole/database": "self.version",
        "lawoole/log": "self.version",
        "lawoole/routing": "self.version",
        "lawoole/server": "self.version",
        "lawoole/swoole": "self.version",
        "lawoole/support": "self.version",
        "lawoole/task": "self.version"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "lawoole/swoole-api": "dev-master",
        "illuminate/validation": "5.5.*",
        "illuminate/view": "5.5.*"
    },
    "autoload": {
        "psr-4": {
            "Lawoole\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
